<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background2">

    <ListView
        android:id="@+id/listViewDica"
        android:layout_width="378dp"
        android:layout_height="154dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="13dp"
        android:layout_marginBottom="14dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="569dp" />

    <TextView
        android:id="@+id/textViewDicas"
        android:layout_width="359dp"
        android:layout_height="42dp"
        android:layout_above="@+id/listViewDica"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="28dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:text="@string/dica"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="506dp" />

    <TextView
        android:id="@+id/textViewSensorLuminosidade"
        android:layout_width="103dp"
        android:layout_height="46dp"
        android:layout_above="@+id/textViewDicas"
        android:layout_alignParentStart="true"
        android:layout_marginStart="9dp"
        android:layout_marginBottom="91dp"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewSensorUmidadeSolo"
        android:layout_width="121dp"
        android:layout_height="41dp"
        android:layout_above="@+id/textViewSensorUmidadeAmbiente"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="23dp"
        android:layout_toStartOf="@+id/textViewSensorTempSolo"
        android:layout_toEndOf="@+id/imageViewSensorLuminosidade"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewSensorUmidadeAmbiente"
        android:layout_width="116dp"
        android:layout_height="39dp"
        android:layout_above="@+id/textViewDicas"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="13dp"
        android:layout_marginBottom="33dp"
        android:layout_toStartOf="@+id/textViewSensorTempAmbiente"
        android:layout_toEndOf="@+id/textViewSensorLuminosidade"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewSensorTempSolo"
        android:layout_width="121dp"
        android:layout_height="44dp"
        android:layout_above="@+id/textViewSensorTempAmbiente"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="19dp"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="288dp"
        tools:layout_editor_absoluteY="301dp" />

    <TextView
        android:id="@+id/textViewSensorTempAmbiente"
        android:layout_width="118dp"
        android:layout_height="45dp"
        android:layout_above="@+id/textViewDicas"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="22dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/rounded_input_text"
        android:gravity="center"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="345dp" />

    <ImageView
        android:id="@+id/imageViewSensorLuminosidade"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:layout_above="@+id/textViewSensorLuminosidade"
        android:layout_alignParentStart="true"
        android:layout_marginStart="17dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/background_logo"
        app:srcCompat="@drawable/light"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="165dp" />

    <ImageView
        android:id="@+id/imageViewSensorUmidade"
        android:layout_width="94dp"
        android:layout_height="90dp"
        android:layout_above="@+id/textViewSensorUmidadeSolo"
        android:layout_marginStart="45dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="30dp"
        android:layout_toStartOf="@+id/imageViewSensorTemperatura"
        android:layout_toEndOf="@+id/imageViewSensorLuminosidade"
        android:background="@drawable/background_logo"
        app:srcCompat="@drawable/gota"
        tools:layout_editor_absoluteX="170dp"
        tools:layout_editor_absoluteY="177dp" />

    <ImageView
        android:id="@+id/imageViewSensorTemperatura"
        android:layout_width="93dp"
        android:layout_height="97dp"
        android:layout_above="@+id/textViewSensorTempSolo"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="236dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="30dp"
        android:background="@drawable/background_logo"
        app:srcCompat="@drawable/temperatura"
        tools:layout_editor_absoluteX="292dp"
        tools:layout_editor_absoluteY="167dp" />

    <ImageView
        android:id="@+id/imageViewSensorImg"
        android:layout_width="179dp"
        android:layout_height="157dp"
        android:layout_above="@+id/imageViewSensorLuminosidade"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="113dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="119dp"
        android:layout_marginBottom="72dp"
        android:background="@drawable/background_imagem"
        app:srcCompat="@drawable/buttonshape"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textViewNomePlantaSensor"
        android:layout_width="336dp"
        android:layout_height="51dp"
        android:layout_above="@+id/imageViewSensorUmidade"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="43dp"
        android:layout_marginBottom="13dp"
        android:gravity="center"
        android:textAlignment="center"
        android:textColor="@color/cardview_light_background"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="21dp"
        tools:layout_editor_absoluteY="345dp" />

</RelativeLayout>